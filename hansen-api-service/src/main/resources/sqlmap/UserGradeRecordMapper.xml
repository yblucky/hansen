<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hansen.mapperUserGradeRecordMapper">

	<sql id="fields">
		userId,recordType,historyGrade,currencyGrade,createTime,updateTime,status,remark,orderNo,amt,upGradeType
	</sql>

	<sql id="fields_id">
		id,<include refid="fields"/>
	</sql>

	<sql id="table_name">base_user_grade_record</sql>

	<sql id="selector">
		select <include refid="fields_id"/> from <include refid="table_name"/>
	</sql>

	<sql id="condition">
		<where>
			<if test="model.userId != null and model.userId != ''">userId=#{model.userId}</if>
			<if test="model.recordType != null">and recordType=#{model.recordType}</if>
			<if test="model.historyGrade != null">and historyGrade=#{model.historyGrade}</if>
			<if test="model.currencyGrade != null">and currencyGrade=#{model.currencyGrade}</if>
			<if test="model.status != null">and status=#{model.status}</if>
			<if test="model.remark != null and model.remark != ''">and remark=#{model.remark}</if>
			<if test="model.orderNo != null and model.orderNo != ''">and remark=#{model.orderNo}</if>
			<if test="model.amt != null">and amt=#{model.amt}</if>
			<if test="model.upGradeType != null and model.upGradeType != ''">and remark=#{model.upGradeType}</if>
		</where>
	</sql>

	<insert id="create" useGeneratedKeys="true" keyProperty="id">
		insert into <include refid="table_name"/>(
		<trim  suffixOverrides="," >
			<if test="model.id!= null  and model.id!=''">
				id,
			</if>
			<if test="model.userId!= null  and model.userId!=''">
				userId,
			</if>
			<if test="model.recordType!= null  and model.recordType!=''">
				recordType,
			</if>
			<if test="model.historyGrade!= null  and model.historyGrade!=''">
				historyGrade,
			</if>
			<if test="model.currencyGrade!= null  and model.currencyGrade!=''">
				currencyGrade,
			</if>
			<if test="model.createTime!= null  and model.createTime!=''">
				createTime,
			</if>
			<if test="model.updateTime!= null  and model.updateTime!=''">
				updateTime,
			</if>
			<if test="model.status!= null  and model.status!=''">
				status,
			</if>
			<if test="model.remark!= null  and model.remark!=''">
				remark,
			</if>
			<if test="model.orderNo!= null  and model.orderNo!=''">
				orderNo,
			</if>
			<if test="model.amt!= null  and model.amt!=''">
				amt,
			</if>
			<if test="model.upGradeType!= null  and model.upGradeType!=''">
				upGradeType,
			</if>
		</trim>
		)
		values(
		<trim  suffixOverrides="," >

			<if test="model.id!= null  and model.id!=''">
				#{model.id},
			</if>
			<if test="model.userId!= null  and model.userId!=''">
				#{model.userId},
			</if>
			<if test="model.recordType!= null  and model.recordType!=''">
				#{model.recordType},
			</if>
			<if test="model.historyGrade!= null  and model.historyGrade!=''">
				#{model.historyGrade},
			</if>
			<if test="model.currencyGrade!= null  and model.currencyGrade!=''">
				#{model.currencyGrade},
			</if>
			<if test="model.createTime!= null  and model.createTime!=''">
				#{model.createTime},
			</if>
			<if test="model.updateTime!= null  and model.updateTime!=''">
				#{model.updateTime},
			</if>
			<if test="model.status!= null  and model.status!=''">
				#{model.status},
			</if>
			<if test="model.remark!= null  and model.remark!=''">
				#{model.remark},
			</if>
			<if test="model.orderNo!= null  and model.orderNo!=''">
				#{model.orderNo},
			</if>
			<if test="model.amt!= null  and model.amt!=''">
				#{model.amt},
			</if>
			<if test="model.upGradeType!= null  and model.upGradeType!=''">
				#{model.upGradeType},
			</if>
		</trim>
		);
	</insert>

	<select id="readById" resultType="UserGradeRecord">
		<include refid="selector"/> where id=#{id}  limit 1;
	</select>

	<select id="readList" resultType="UserGradeRecord">
		<include refid="selector"/> <include refid="condition"/> limit #{startRow}, #{pageSize};
	</select>

	<select id="readOne" resultType="UserGradeRecord">
		<include refid="selector"/> <include refid="condition"/> limit 1;
	</select>

	<select id="readCount" resultType="int">
		select count(1) from <include refid="table_name"/> <include refid="condition"/>
	</select>

	<update id="updateById">
		update <include refid="table_name"/>
		<set>
			<if test="model.userId != null and model.userId !=''">userId= #{model.userId},</if>
			<if test="model.recordType != null">recordType= #{model.recordType},</if>
			<if test="model.historyGrade != null">historyGrade= #{model.historyGrade},</if>
			<if test="model.currencyGrade != null">currencyGrade= #{model.currencyGrade},</if>
			<if test="model.status != null">status= #{model.status},</if>
			<if test="model.remark != null and model.remark !=''">remark= #{model.remark},</if>
			<if test="model.orderNo != null and model.orderNo !=''">orderNo= #{model.orderNo},</if>
			<if test="model.amt != null and model.amt !=''">amt= #{model.amt},</if>
			<if test="model.upGradeType != null and model.upGradeType !=''">upGradeType= #{model.upGradeType},</if>
			updateTime=now()
		</set>
		where id=#{id}
	</update>

	<update id="deleteById">
		update <include refid="table_name"/> set status = 0,updateTime=now() where id = #{id}
	</update>

</mapper>



